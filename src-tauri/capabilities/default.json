{
  "identifier": "default",
  "description": "Default capabilities for mdjax",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:window:allow-set-title",
    "core:window:allow-close",
    "core:path:default",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "dialog:allow-ask",
    "fs:default",
    "fs:allow-home-read-recursive",
    "fs:allow-home-write-recursive",
    "window-state:default"
  ]
}
